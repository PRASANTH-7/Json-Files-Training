import json

# Sample data to be converted to JSON
data = {
    "name": "John Doe",
    "age": 30,
    "city": "New York",
    "is_student": False,
    "courses": ["Math", "Science", "History"]
}

# Dumping data to JSON string
json_string = json.dumps(data, indent=4)
print("JSON String:")
print(type(json_string))

# Loading data from JSON string
loaded_data = json.loads(json_string)
print("\nLoaded Data:")
print(loaded_data)
